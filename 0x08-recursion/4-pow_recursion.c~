#include "main.h"
#include <stdio.h>
/**
 *_pow_recursion - Entry point
 *@x:  is the int that will use for the argument of the function
 *@y: another int
 *Return: Always 0 (Success)
 */
int _pow_recursion(int x, int y)
{
if (y < 0)
{
return (-1);
}
else
{
return (x * _pow_recursion(x, y - 1));
}
}
