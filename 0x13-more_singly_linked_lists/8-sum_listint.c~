#include "lists.h"
/**
 * free_listint2 - this function frees a list
 * @head: the pointer to the struct
 *
 * Return: the number of elements
 */
int sum_listint(listint_t *head)
{
int sum = 0;
listint_t *p;
if (head == NULL)
{
return (0);
}
p = head;
while (p != NULL)
{
sum = sum + p->n;
p = p->next;
}
return (sum);
}
