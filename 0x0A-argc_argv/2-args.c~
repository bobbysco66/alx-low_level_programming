#include "main.h"
#include <stdio.h>
/**
 * main - writes the character c to stdout
 * @argc: The number of arguments
 * @argv: the strings in an array form
 *Return: On success 1.
 */
int main(int argc, char *argv[]) 
{
int i;  
for (i = 0; i < argc; i++;)
{
printf("%s\n", argv[i]);
}
return (0);
}
